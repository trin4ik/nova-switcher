{
    "name": "trin4ik/nova-switcher",
    "description": "A Laravel Nova switcher field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Trin4ik\\NovaSwitcher\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trin4ik\\NovaSwitcher\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
